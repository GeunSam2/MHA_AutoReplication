{
  "id": "/prod/db/myna-proxy",
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "constraints": [
    [
      "hostname",
      "GROUP_BY",
      "2"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 33406,
        "hostPort": 23406,
        "labels": {
          "VIP_0": "/prod/db/myna-proxy:23406"
        },
        "protocol": "tcp",
        "servicePort": 23406,
        "name": "33406"
      },
      {
        "containerPort": 33408,
        "hostPort": 23408,
        "labels": {
          "VIP_1": "/prod/db/myna-proxy:23408"
        },
        "protocol": "tcp",
        "servicePort": 23408,
        "name": "33408"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "geunsam2/haproxy-mynadb-prod:m1v1",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.1,
  "disk": 0,
  "instances": 2,
  "maxLaunchDelaySeconds": 300,
  "mem": 128,
  "gpus": 0,
  "networks": [
    {
      "name": "dcos-2",
      "mode": "container"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "fetch": []
}
